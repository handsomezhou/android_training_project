package bin;

public class Arrays {

	/**
	 * @param args
	 */
	public static int int_arrays()
	{
		Games game[][]=new Games[3][4];
		for(int y=0; y<3; y++){
			for(int x=0; x<4; x++){
				game[y][x].setYX(y,x);
				/*
				game[y][x]=new Games(y, x);
				game[y][x].showYX();
				*/
			}
		}
		return 0;
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Games games=new Games(4, 4);
		//System.out.println(int_arrays(games)[1][1]);;
		int_arrays();
		
	}
	
}
